{
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "lib": ["es2020"],
       "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
       "moduleResolution": "node",
      "strict": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "outDir": "dist"
    },
     "include": ["*.ts"]
  }